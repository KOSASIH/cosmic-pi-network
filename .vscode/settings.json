{
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "html",
        "vue"
    ],
    "python.pythonPath": "venv/bin/python",
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": false,
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=120",
        "--ignore=E501"
    ],
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./tests",
        "-p",
        "test_*.py"
    ],
    "python.testing.nosetestsArgs": [
        "--with-coverage",
        "--cover-package=my_package"
    ],
    "python.testing.pytestArgs": [
        "-v",
        "--cov=my_package"
    ],
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestions": true,
    "editor.quickSuggestions": {
        "strings": true,
        "comments": true,
        "other": true
    },
    "editor.suggest.showUnits": true,
    "editor.suggest.showColors": true,
    "editor.suggest.showKeywords": true,
    "editor.suggest.showOperators": true,
    "editor.suggest.showSnippets": true,
    "editor.suggest.showDeprecated": true,
    "editor.suggest.showDocumentation": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.maxVisibleSuggestions": 10,
    "editor.suggest.snippetSuggestions": "top",
    "editor.suggest.filteredTypes": [
        "variable",
        "constant",
        "function",
        "method",
        "unit",
        "value",
        "object",
        "type",
        "interface",
        "struct",
        "class",
        "enum",
        "module",
        "property",
        "event",
        "operator",
        "keyword",
        "snippet"
    ],
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": true,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fill",
    "editor.minimap.viewPosition": 0,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.side": "right",
    "editor.minimap.scrollbarSize": 8,
    "editor.minimap.useLayerHinting": true,
    "editor.minimap.showFolderOpenMarkers": true,
    "editor.minimap.showRefs": true,
    "editor.minimap.showSelectionHighlights": true,
